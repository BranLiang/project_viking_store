<h4>ID: <%= @category.id %></h4>
<h2><%= @category.name %></h2>
<p>
  Description: <%= @category.description %>
</p>
<%= link_to "Edit", edit_category_path(@category) %>

<div class="list-group">
  <% @products.each do |product| %>
    <%= link_to "ID: #{product.id} #{product.name}", "#", class: "list-group-item" %>
  <% end %>
</div>
